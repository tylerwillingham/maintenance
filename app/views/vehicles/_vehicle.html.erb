<tr>
  <td>
    <%= vehicle.year %>
  </td>
  <td>
    <%= vehicle.make %>
  </td>
  <td>
    <%= vehicle.model %>
  </td>
  <td>
    <%= number_with_delimiter vehicle.mileage %>
  </td>
  <td>
    <% if vehicle.currently_owned? %>
      <%=t :yes %>
    <% else %>
      <%=t :no %>
    <% end %>
  </td>
  <td>
    <%= link_to t(:edit), edit_vehicle_path(vehicle.id) %>
    <%= link_to t(:delete), vehicle_path(vehicle.id), method: :delete %> 
  </td>
</tr>
